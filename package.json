{
  "name": "meshblu-connector-dashboard-opener",
  "version": "1.0.3",
  "description": "Meshblu Connector Powermate",
  "homepage": "https://github.com/octoblu/meshblu-connector-dashboard-opener",
  "bugs": "https://github.com/octoblu/meshblu-connector-dashboard-opener/issues",
  "author": {
    "name": "Octoblu",
    "url": "https://github.com/octoblu"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/octoblu/meshblu-connector-dashboard-opener.git"
  },
  "scripts": {
    "build": "coffee --compile --map --output lib/ src/",
    "start": "meshblu-connector",
    "test": "exit 0",
    "coverage": "nyc npm test",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "bin": "command.js",
  "devDependencies": {
    "chai": "^4.0.2",
    "coffee-coverage": "^2.0.1",
    "mocha": "^3.4.2",
    "mocha-multi": "^0.11.0",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^11.0.2",
    "sinon": "^2.3.5",
    "sinon-chai": "^2.11.0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "meshblu-config": "^8.0.1",
    "meshblu-connector-cli": "^1.1.6",
    "opener": "^1.4.3"
  },
  "meshbluConnector": {
    "githubSlug": "octoblu/meshblu-connector-dashboard-opener"
  },
  "platformDependencies": {
    "darwin": [],
    "win32": [],
    "win64": [],
    "linux": [],
    "android": null,
    "ios": null
  },
  "license": "MIT",
  "keywords": [
    "meshblu-connector",
    "gateblu"
  ],
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  },
  "pkg": {
    "scripts": [
      "package.json",
      "index.js",
      "src/*.js"
    ]
  }
}
